<script>
    AOS.init({
        duration: 1000,
        // startEvent: 'load',
        once:true,
    });
</script>

<script>
    $(function() {
        var header = $(".dg-header");
        $(window).scroll(function() {
            var scroll = $(window).scrollTop();

            if (scroll >= 40) {
                header.addClass("dg-scroll-header");
            } else {
                header.removeClass("dg-scroll-header");
            }
        });
    });

</script>
<script>
    $("#contactbtn").on('click', function(event) {

    var target = $(this.getAttribute('href'));
    console.log(target);
    if( target.length ) {
        event.preventDefault();
        $('html, body').stop().animate({
        scrollTop: target.offset().top
        }, 1000);
    }

    });

</script>